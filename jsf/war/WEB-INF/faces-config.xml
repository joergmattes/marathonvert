<?xml version='1.0' encoding='UTF-8'?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
              version="2.0">
  <application>
    <locale-config>
      <default-locale>en</default-locale>
    </locale-config>
  </application>
  <managed-bean>
  <managed-bean-name>registrationBean</managed-bean-name>
  <managed-bean-class>com.joerg.ui.RegistrationBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  <managed-bean>
  <managed-bean-name>newRegistrationsBean</managed-bean-name>
  <managed-bean-class>com.joerg.ui.NewRegistrationsBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  <managed-bean>
  <managed-bean-name>participantsBean</managed-bean-name>
  <managed-bean-class>com.joerg.ui.ParticipantsBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  <managed-bean>
  <managed-bean-name>allRegistrationsBean</managed-bean-name>
  <managed-bean-class>com.joerg.ui.AllRegistrationsBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  <managed-bean>
  <managed-bean-name>bookBean2</managed-bean-name>
  <managed-bean-class>com.joerg.ui.BookBean2</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
  </managed-bean>
  <managed-bean>
  <managed-bean-name>bedsBean</managed-bean-name>
  <managed-bean-class>com.joerg.ui.BedsBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
  <managed-bean>
  <managed-bean-name>copyAllApplicantsBean</managed-bean-name>
  <managed-bean-class>com.joerg.ui.tmp.CopyAllApplicantsBean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
  </managed-bean>
  <navigation-rule>
  	<navigation-case>
  		<from-outcome>confirmRegistration</from-outcome>
  		<to-view-id>/site/confirmRegistration.xhtml</to-view-id>
  		<redirect/>
  	</navigation-case>
  	<navigation-case>
  		<from-outcome>step1</from-outcome>
  		<to-view-id>/site/book/step1.xhtml</to-view-id>
  	</navigation-case>
  	<navigation-case>
  		<from-outcome>step2</from-outcome>
  		<to-view-id>/site/book/step2.xhtml</to-view-id>
  	</navigation-case>
  	<navigation-case>
  		<from-outcome>end</from-outcome>
  		<to-view-id>/site/book/end.xhtml</to-view-id>
  		<redirect/>
  	</navigation-case>
  	<navigation-case>
  		<from-outcome>cancel</from-outcome>
  		<to-view-id>/site/book/cancelBook.xhtml</to-view-id>
  		<redirect/>
  	</navigation-case>
  	<navigation-case>
  		<from-outcome>participants</from-outcome>
  		<to-view-id>/site/jaroussky/participants.xhtml</to-view-id>
  		<redirect/>
  	</navigation-case>
  	<navigation-case>
  		<from-outcome>newRegistrations</from-outcome>
  		<to-view-id>/site/jaroussky/newRegistrations.xhtml</to-view-id>
  		<redirect/>
  	</navigation-case>
  </navigation-rule>
</faces-config>